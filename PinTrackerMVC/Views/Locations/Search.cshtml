

@using PinTrackerMVC.Models
@* @model List<Location> *@

<h2>LOCATION RESULTS:</h2>

@foreach (Location location in Model)
{
  <h3>Venue: @location.name</h3>
  <h4>Address:</h4>
  <p>@location.street</p>
  <p>@location.city , @location.state</p>
  <p>@location.zip</p>
  <p>@location.phone</p>
  <p>@location.num_machines</p>
  <hr>
  
}